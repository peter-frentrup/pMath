// You should pre-define PMATH_DECLARE_SYMBOL(sym, name_str, attrib)

#define PMATH_DECLARE_SYSTEM_SYMBOL(name, attrib) \
  PMATH_DECLARE_SYMBOL( \
    pmath_System_ ## name, \
    "System`" #name, \
    PMATH_SYMBOL_ATTRIBUTE_PROTECTED | (attrib) \
  )

#define PMATH_DECLARE_SYSTEM_PRIVATE_SYMBOL(name, attrib) \
  PMATH_DECLARE_SYMBOL( \
    pmath_System_Private_ ## name, \
    "System`Private`" #name, \
    PMATH_SYMBOL_ATTRIBUTE_PROTECTED | (attrib) \
  )

