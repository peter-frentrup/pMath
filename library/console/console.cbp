<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="console" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows-Debug">
				<Option platforms="Windows;" />
				<Option output="bin/windows/debug/pmathc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/windows/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-march=i686" />
					<Add option="-g" />
					<Add option="-mthreads" />
					<Add option="-mms-bitfields" />
					<Add option="-Wdeclaration-after-statement" />
					<Add option="-Wpointer-arith" />
				</Compiler>
				<Linker>
					<Add option="-static-libgcc" />
					<Add library="pmath" />
					<Add directory="../bin/windows/debug" />
				</Linker>
			</Target>
			<Target title="Windows-Release">
				<Option platforms="Windows;" />
				<Option output="bin/windows/release/pmathc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/windows/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-march=i686" />
					<Add option="-O2" />
					<Add option="-mthreads" />
					<Add option="-mms-bitfields" />
					<Add option="-Wdeclaration-after-statement" />
					<Add option="-Wpointer-arith" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static-libgcc" />
					<Add library="pmath" />
					<Add directory="../bin/windows/release" />
				</Linker>
			</Target>
			<Target title="Windows-MSVC">
				<Option platforms="Windows;" />
				<Option output="bin/windows/msvc/pmathc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/windows/msvc/" />
				<Option type="1" />
				<Option compiler="msvc10" />
				<Compiler>
					<Add option="/MT" />
					<Add option="/arch:SSE2" />
					<Add option="/Gd" />
					<Add option="/GL" />
					<Add option="/GF" />
					<Add option="/Ox" />
					<Add option="/W3" />
					<Add option="/wd4711" />
					<Add option="/DNDEBUG" />
					<Add directory="../../msvc-c99" />
				</Compiler>
				<Linker>
					<Add library="pmath.lib" />
					<Add directory="../bin/windows/msvc" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="../include" />
			<Add directory="$(#editline.include)" />
		</Compiler>
		<Linker>
			<Add directory="$(#editline.lib)" />
		</Linker>
		<Unit filename="src/console.h" />
		<Unit filename="src/input.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger>
				<remote_debugging>
					<options conn_type="0" serial_baud="115200" additional_cmds="python import sys&#x0A;python sys.path.insert(0, &apos;../../gdb&apos;)&#x0A;python import pmath_gdb&#x0A;python pmath_gdb.printers.register_pmath_printers(None)" />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
