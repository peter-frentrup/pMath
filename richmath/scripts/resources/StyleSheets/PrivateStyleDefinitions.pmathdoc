          "Docked")}}, FontFeatures -> {"ssty" -> Automatic}, 
       Magnification -> 1, Visible -> True, WindowFrame -> "Normal", 
       WindowTitle -> Dynamic(
        ToString(Row({"Private StyleDefinitions for ", 
          With({Global`owner:= CurrentValue("StyleDefinitionsOwner")}, 
           With({Global`title:= CurrentValue(Global`owner, WindowTitle)}, 
