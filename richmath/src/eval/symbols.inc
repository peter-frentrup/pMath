RICHMATH_DECLARE_FE_SYMBOL(   NumberBox )
RICHMATH_DECLARE_FE_SYMBOL(   SymbolInfo )
RICHMATH_DECLARE_FE_SYMBOL(   AddConfigShaper )
RICHMATH_DECLARE_FE_SYMBOL_2( InternalExecuteFor , PMATH_SYMBOL_ATTRIBUTE_HOLDFIRST)

RICHMATH_DECLARE_FE_SYMBOL( SymbolDefinitions )

RICHMATH_DECLARE_FE_SYMBOL( FileOpenDialog )
RICHMATH_DECLARE_FE_SYMBOL( FileSaveDialog )
RICHMATH_DECLARE_FE_SYMBOL( ColorDialog )
RICHMATH_DECLARE_FE_SYMBOL( FontDialog )

RICHMATH_DECLARE_FE_DOLLAR_SYMBOL( ControlActive )
