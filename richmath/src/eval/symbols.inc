/* You should pre-define RICHMATH_DECLARE_SYMBOL(sym, name_str)
   and RICHMATH_RESET_SYMBOL_ATTRIBUTES(sym, attr).
   You may pre-define RICHMATH_IMPL_GUI(sym, name_str, cppfunc)
*/

/*
#define RICHMATH_IMPL_GUI_FUNCTION(FUNCNAME) \
  static pmath_t builtin_ ## FUNCNAME(pmath_expr_t expr) { \
    if(!Application::is_running_on_gui_thread()) \
      return expr; \
    return gui_builtin_ ## FUNCNAME( Expr{ expr } ).release(); \
  }*/

#ifdef RICHMATH_IMPL_GUI
#  define RICHMATH_GUI_SYMBOL(SYM, NAME, CPPFUNC)  RICHMATH_DECLARE_SYMBOL(SYM, NAME)  RICHMATH_IMPL_GUI(SYM, NAME, CPPFUNC)
#else
#  define RICHMATH_GUI_SYMBOL(SYM, NAME, CPPFUNC)  RICHMATH_DECLARE_SYMBOL(SYM, NAME)
#endif

#define RICHMATH_DECLARE_SYSTEM_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_System_ ## NAME, \
    "System`" #NAME \
  )

#define RICHMATH_DECLARE_SYSTEM_DOLLAR_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_System_Dollar ## NAME, \
    "System`$" #NAME \
  )

#define RICHMATH_DECLARE_SYSTEM_PRIVATE_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_System_Private_ ## NAME, \
    "System`Private`" #NAME \
  )

#define RICHMATH_DECLARE_FE_DOLLAR_VARIABLE(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_FE_Dollar ## NAME, \
    "FE`$" #NAME \
  ) \
  RICHMATH_RESET_SYMBOL_ATTRIBUTES( \
    richmath_FE_Dollar ## NAME, \
    0 \
  )

#define RICHMATH_DECLARE_FE_STYLES_DOLLAR_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_FE_Styles_Dollar ## NAME, \
    "FE`Styles`$" #NAME \
  )
  
#define RICHMATH_DECLARE_FE_SYMBOL_2(NAME, ATTR) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_FE_ ## NAME, \
    "FE`" #NAME \
  ) \
  RICHMATH_RESET_SYMBOL_ATTRIBUTES( \
    richmath_FE_ ## NAME, \
    PMATH_SYMBOL_ATTRIBUTE_PROTECTED | (ATTR)\
  )
#define RICHMATH_DECLARE_FE_SYMBOL(NAME) \
  RICHMATH_DECLARE_FE_SYMBOL_2(NAME, 0)

#define RICHMATH_DECLARE_FRONTEND_CPPFUNC(NAME) \
  RICHMATH_GUI_SYMBOL( \
    richmath_FrontEnd_ ## NAME, \
    "FrontEnd`" #NAME, \
    richmath_eval_FrontEnd_ ## NAME \
  ) \
  RICHMATH_RESET_SYMBOL_ATTRIBUTES( \
    richmath_FrontEnd_ ## NAME, \
    PMATH_SYMBOL_ATTRIBUTE_PROTECTED \
  )

#define RICHMATH_DECLARE_DEVELOPER_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_Developer_ ## NAME, \
    "Developer`" #NAME \
  )

#define RICHMATH_DECLARE_DOCUMENTATION_SYMBOL(NAME) \
  RICHMATH_DECLARE_SYMBOL( \
    richmath_Documentation_ ## NAME, \
    "Documentation`" #NAME \
  )

