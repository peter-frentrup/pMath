<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hello" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows-Debug">
				<Option platforms="Windows;" />
				<Option output="bin\windows\hello" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\windows\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="..\..\library\test\bin\windows\debug\test.exe" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-march=i686" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-static-libgcc" />
					<Add library="pmath" />
					<Add directory="..\..\library\bin\windows\debug" />
				</Linker>
			</Target>
			<Target title="Linux-Debug">
				<Option platforms="Unix;" />
				<Option output="bin\linux\hello" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\linux\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-march=i686" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-static-libgcc" />
					<Add library="pmath" />
					<Add directory="..\..\library\bin\linux\debug" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="..\..\library\include" />
		</Compiler>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
