				</Linker>
			</Target>
			<Target title="Windows-Release">
				<Option platforms="Windows;" />
				<Option output="bin\pmath-eigen" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-march=i686" />
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-fno-exceptions" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static-libgcc" />
					<Add directory="..\..\library\bin\windows\release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="..\..\library\include" />
