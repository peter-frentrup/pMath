  P4E_DECLARE_SYMBOL( \
    p4e_System_Dollar ## name, \
    "System`$" #name \
  )
  
#define P4E_DECLARE_EIGEN_SYMBOL(name) \
  P4E_DECLARE_SYMBOL( \
    p4e_Eigen_ ## name, \
    "Eigen`" #name \
  )

